{
  "name": "EventMagic QR Handler",
  "short_name": "QRHandler",
  "start_url": "/index.html",
  "scope": "/",
    "scope_url": "https://qr.eventmagic.co/",
  "intent_filters": {
    "scope_url_scheme": "https",
    "scope_url_host": "qr.eventmagic.co",
    "scope_url_path": "/"
  },
  "display": "standalone",
  "background_color": "#ffffff",
  "theme_color": "#000000",
  "icons": [
    {
      "src": "/icons/icon-192x192.png",
      "sizes": "192x192",
      "type": "image/png"
    },
    {
      "src": "/icons/icon-512x512.png",
      "sizes": "512x512",
      "type": "image/png"
    }
  ]
}
